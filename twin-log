# Make the command executable locally
chmod +x twin-log
from datasets import load_dataset

# 1. Load the dataset from the market
dataset = load_dataset("your-username/debris-digital-twin-2026")

# 2. Access a sample to view the Digital Twin metadata
sample = dataset['train'][0]
print(f"Material: {sample['metadata']['material_type']}")
print(f"Condition: {sample['metadata']['condition']}")

# 3. Calculate the 2076 Forecast based on our adaptive strategy
impact_score = sample['metadata']['environmental_impact_score']
print(f"Environmental Impact Score: {impact_score}/10")

if impact_score > 7:
    print("Warning: High persistent debris risk for 50-year horizon.")
